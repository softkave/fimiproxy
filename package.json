{
  "name": "fimiproxy",
  "version": "0.1.2",
  "description": "simple HTTP and HTTPS reverse proxy",
  "author": {
    "email": "ywordk@gmail.com",
    "name": "softkave"
  },
  "license": "MIT",
  "keywords": [
    "reverse proxy",
    "http",
    "https",
    "host"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/softkave/fimiproxy.git"
  },
  "bugs": {
    "url": "https://github.com/softkave/fimiproxy/issues"
  },
  "homepage": "https://github.com/softkave/fimiproxy",
  "engines": {
    "node": "21.x",
    "npm": "10.x"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "bin": "build/src/bin.js",
  "files": [
    "build/src/!(__tests__|testutils|private)*(js|d.ts)"
  ],
  "scripts": {
    "test": "cross-env NODE_TLS_REJECT_UNAUTHORIZED='0' jest --config=jest.config.ts --detectOpenHandles",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "publish-to-npm": "npm run compile && npm publish",
    "show-packlist": "npm pack --dry-run"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^28.1.6",
    "@types/node": "20.8.2",
    "@types/node-forge": "^1.3.11",
    "cross-env": "^7.0.3",
    "express": "^4.18.3",
    "gts": "^5.2.0",
    "jest": "^28.1.3",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.0"
  }
}
